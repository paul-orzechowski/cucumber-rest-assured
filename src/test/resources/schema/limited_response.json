{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "timestamp": {
      "type": "integer"
    },
    "base": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "rates": {
      "type": "object",
      "properties": {
        "GBP": {
          "type": "number"
        },
        "JPY": {
          "type": "number"
        },
        "USD": {
          "type": "number"
        }
      },
      "required": [
        "GBP",
        "JPY",
        "USD"
      ]
    }
  },
  "required": [
    "success",
    "timestamp",
    "base",
    "date",
    "rates"
  ]
}